      <max value="1"/>
      <type>
        <code value="Reference"/>
        <targetProfile
          value="http://ns.electronichealth.net.au/ci/fhir/3.0/StructureDefinition/encounter-es-1"/>
      </type>
      <mustSupport value="true"/>
    </element>

    <!-- Allergies Section -->
    <element id="Composition.section:allergies">
      <extension
        url="http://hl7.org/fhir/StructureDefinition/structuredefinition-explicit-type-name">
        <valueString value="Section"/>
      </extension>
      <path value="Composition.section"/>
      <sliceName value="allergies"/>
      <short value="Allergies"/>
      <definition
        value="Information about allergies and intolerances of the patient. This includes statements that a patient does not have an allergy or category of allergies."/>
      <min value="0"/>
      <max value="1"/>
      <constraint>
        <key value="inv-dh-cmp-07"/>
        <severity value="error"/>
        <human value="The section shall at least have one entry or an empty reason, but not both"/>
        <expression
          value="Composition.section.where($this is entry).exists() xor Composition.section.where($this is emptyReason).exists()"
        />
      </constraint>
      <mustSupport value="true"/>
    </element>
    <element id="Composition.section:allergies.title">
      <path value="Composition.section.title"/>
      <min value="1"/>
      <fixedString value="Allergies and Adverse Reactions"/>
      <mustSupport value="true"/>
    </element>
    <element id="Composition.section:allergies.code">
      <path value="Composition.section.code"/>
      <min value="1"/>
      <fixedCodeableConcept>
        <coding>
          <system value="http://loinc.org"/>
          <code value="48765-2"/>
        </coding>
      </fixedCodeableConcept>
      <mustSupport value="true"/>
    </element>
    <element id="Composition.section:allergies.text">
      <path value="Composition.section.text"/>
      <min value="1"/>
      <mustSupport value="true"/>
    </element>
    <element id="Composition.section:allergies.mode">
      <path value="Composition.section.mode"/>
      <fixedCode value="snapshot"/>
    </element>
    <element id="Composition.section:allergies.entry">
      <path value="Composition.section.entry"/>
      <short value="Allergies and/or intolerances"/>
      <type>
        <code value="Reference"/>
        <targetProfile
          value="http://ns.electronichealth.net.au/ci/fhir/3.0/StructureDefinition/allergyintolerance-summary-1"
        />
      </type>
      <mustSupport value="true"/>
    </element>
    <element id="Composition.section:allergies.emptyReason">
      <path value="Composition.section.emptyReason"/>
      <mustSupport value="true"/>
      <binding>
        <extension url="http://hl7.org/fhir/StructureDefinition/elementdefinition-bindingName">
          <valueString value="SectionEmptyReason"/>
        </extension>
        <strength value="required"/>
        <valueSetReference>
          <reference
            value="https://healthterminologies.gov.au/fhir/ValueSet/non-clinical-empty-reason-1"/>
        </valueSetReference>
      </binding>
    </element>

    <!-- Medications Section -->
    <element id="Composition.section:medications">
      <extension
        url="http://hl7.org/fhir/StructureDefinition/structuredefinition-explicit-type-name">
        <valueString value="Section"/>
      </extension>
      <path value="Composition.section"/>
      <sliceName value="medications"/>
      <short value="Medications"/>
      <definition
        value="Information about medicines that are relevant to the encounter. The medicines included do not constitute a full medications list, but are those medicines that have specifically changed as a result of the encounter, or those medicines directly relevant to the encounter."/>
      <min value="0"/>
      <max value="1"/>
      <constraint>
        <key value="inv-dh-cmp-08"/>
        <severity value="error"/>
        <human value="The section shall at least have one entry or an empty reason, but not both"/>
        <expression
          value="Composition.section.where($this is entry).exists() xor Composition.section.where($this is emptyReason).exists()"
        />
      </constraint>
      <mustSupport value="true"/>
    </element>
    <element id="Composition.section:medications.title">
      <path value="Composition.section.title"/>
      <min value="1"/>
      <fixedString value="Medications"/>
      <mustSupport value="true"/>
    </element>
    <element id="Composition.section:medications.code">
      <path value="Composition.section.code"/>
      <min value="1"/>
      <fixedCodeableConcept>
        <coding>
          <system value="http://loinc.org"/>
          <code value="10160-0"/>
        </coding>
      </fixedCodeableConcept>
      <mustSupport value="true"/>
    </element>
    <element id="Composition.section:medications.text">
      <path value="Composition.section.text"/>
      <min value="1"/>
      <mustSupport value="true"/>
    </element>
    <element id="Composition.section:medications.mode">
      <path value="Composition.section.mode"/>
      <fixedCode value="snapshot"/>
    </element>
    <element id="Composition.section:medications.entry">
      <path value="Composition.section.entry"/>
      <short value="Medications"/>
      <min value="0"/>
      <max value="1"/>
      <type>
        <code value="Reference"/>
        <targetProfile
          value="http://ns.electronichealth.net.au/ci/fhir/3.0/StructureDefinition/list-medseventchanges-1"
        />
      </type>
      <type>
        <code value="Reference"/>
        <targetProfile
          value="http://ns.electronichealth.net.au/ci/fhir/3.0/StructureDefinition/observation-norelevantfinding-1"
        />
      </type>
      <constraint>
        <key value="inv-dh-cmp-03"/>
        <severity value="error"/>
        <human
          value="A list of medication changes from this event or an assertion that there are no current medications shall be present, but not both"/>
        <expression
          value="Composition.section.where($this.code.coding.system='http://loinc.org' and $this.code.coding.code = '10160-0').exists() implies (Composition.section.where($this.code.coding.system='http://loinc.org' and $this.code.coding.code = '10160-0').entry.resolve().where($this is List).exists() xor Composition.section.where($this.code.coding.system='http://loinc.org' and $this.code.coding.code = '10160-0').entry.resolve().where($this is Observation ).exists())"
        />
      </constraint>
      <mustSupport value="true"/>
    </element>
    <element id="Composition.section:medications.emptyReason">
      <path value="Composition.section.emptyReason"/>
      <mustSupport value="true"/>
      <binding>
        <extension url="http://hl7.org/fhir/StructureDefinition/elementdefinition-bindingName">
          <valueString value="SectionEmptyReason"/>
        </extension>
        <strength value="required"/>
        <valueSetReference>
          <reference
            value="https://healthterminologies.gov.au/fhir/ValueSet/non-clinical-empty-reason-1"/>
        </valueSetReference>
      </binding>
    </element>

    <!-- Medical History Section -->
    <element id="Composition.section:medicalHistory">
      <extension
        url="http://hl7.org/fhir/StructureDefinition/structuredefinition-explicit-type-name">
        <valueString value="Section"/>
      </extension>
      <path value="Composition.section"/>
      <sliceName value="medicalHistory"/>
      <short value="Medical History"/>
      <definition
        value="Information about the problems, diagnoses and medical or surgical procedures of a patient. This can include statements that a patient does not have a particular condition."/>
      <min value="0"/>
      <max value="1"/>
      <constraint>
        <key value="inv-dh-cmp-09"/>
        <severity value="error"/>
        <human value="The section shall at least have one entry or an empty reason, but not both"/>
        <expression
          value="Composition.section.where($this is entry).exists() xor Composition.section.where($this is emptyReason).exists()"
        />
      </constraint>
      <mustSupport value="true"/>
    </element>
    <element id="Composition.section:medicalHistory.title">
      <path value="Composition.section.title"/>
      <min value="1"/>
      <fixedString value="Diagnoses/Interventions"/>
      <mustSupport value="true"/>
    </element>
    <element id="Composition.section:medicalHistory.code">
      <path value="Composition.section.code"/>
      <min value="1"/>
      <fixedCodeableConcept>
        <coding>
          <system value="https://healthterminologies.gov.au/fhir/CodeSystem/nctis-data-components-1"/>
          <code value="101.16117"/>
        </coding>
      </fixedCodeableConcept>
      <mustSupport value="true"/>
    </element>
    <element id="Composition.section:medicalHistory.text">
      <path value="Composition.section.text"/>
      <min value="1"/>
      <mustSupport value="true"/>
    </element>
    <element id="Composition.section:medicalHistory.mode">
      <path value="Composition.section.mode"/>
      <fixedCode value="snapshot"/>
    </element>
    <element id="Composition.section:medicalHistory.entry">
      <path value="Composition.section.entry"/>
      <short value="Medical history"/>
      <type>
        <code value="Reference"/>
        <targetProfile
          value="http://ns.electronichealth.net.au/ci/fhir/3.0/StructureDefinition/condition-summary-1"
        />
      </type>
      <type>
        <code value="Reference"/>
        <targetProfile
          value="http://ns.electronichealth.net.au/ci/fhir/3.0/StructureDefinition/procedure-summary-1"
        />
      </type>
      <type>
        <code value="Reference"/>
        <targetProfile
          value="http://ns.electronichealth.net.au/ci/fhir/3.0/StructureDefinition/observation-norelevantfinding-1"
        />
      </type>
      <constraint>
        <key value="inv-dh-cmp-04"/>
        <severity value="error"/>
        <human
          value="At least one condition or procedure shall be present, or an assertion that there is no history shall be present, but not both"/>
        <expression
          value="Composition.section.where($this.code.coding.system = 'https://healthterminologies.gov.au/fhir/CodeSystem/nctis-data-components-1' and $this.code.coding.code = '101.16117').entry.resolve().where($this is Observation).exists() implies (Composition.section.where($this.code.coding.system = 'https://healthterminologies.gov.au/fhir/CodeSystem/nctis-data-components-1' and $this.code.coding.code = '101.16117').entry.resolve().where($this is Observation).count()&lt;2 and Composition.section.where($this.code.coding.system = 'https://healthterminologies.gov.au/fhir/CodeSystem/nctis-data-components-1' and $this.code.coding.code = '101.16117').entry.resolve().where($this is value and value.coding.code = '1224831000168103').exists())"
        />
      </constraint>
      <constraint>
        <key value="inv-dh-cmp-12" />
        <severity value="error" />
        <human value="This section shall contain at most one assertion of no relevant finding entry and it shall assert no relevant medical history" />
        <expression value="Composition.section.where($this.code.coding.system = 'https://healthterminologies.gov.au/fhir/CodeSystem/nctis-data-components-1' and $this.code.coding.code = '101.16117').entry.resolve().where($this is Observation).exists() implies (Composition.section.where($this.code.coding.system = 'https://healthterminologies.gov.au/fhir/CodeSystem/nctis-data-components-1' and $this.code.coding.code = '101.16117').entry.resolve().where($this is Observation).count()&lt;2 and Composition.section.where($this.code.coding.system = 'https://healthterminologies.gov.au/fhir/CodeSystem/nctis-data-components-1' and $this.code.coding.code = '101.16117').entry.resolve().where($this is value and value.coding.code = '1224831000168103').exists())" />
      </constraint>
      <mustSupport value="true"/>
    </element>
    <element id="Composition.section:medicalHistory.emptyReason">
      <path value="Composition.section.emptyReason"/>
      <mustSupport value="true"/>
      <binding>
        <extension url="http://hl7.org/fhir/StructureDefinition/elementdefinition-bindingName">
          <valueString value="SectionEmptyReason"/>
        </extension>
        <strength value="required"/>
        <valueSetReference>
          <reference
            value="https://healthterminologies.gov.au/fhir/ValueSet/non-clinical-empty-reason-1"/>
        </valueSetReference>
      </binding>
    </element>

    <!-- Immunizations Section -->
    <element id="Composition.section:immunisations">
      <extension
        url="http://hl7.org/fhir/StructureDefinition/structuredefinition-explicit-type-name">
        <valueString value="Section"/>
      </extension>
      <path value="Composition.section"/>
      <sliceName value="immunisations"/>
      <short value="Immunisations"/>
      <definition
        value="Information about vaccinations administered or reported to be administered during this encounter. This may include statements that a patient has not had a particular vaccine administered."/>
      <min value="0"/>
      <max value="1"/>
      <constraint>
        <key value="inv-dh-cmp-10"/>
        <severity value="error"/>
        <human value="The section shall at least have one entry or an empty reason, but not both"/>
        <expression
          value="Composition.section.where($this is entry).exists() xor Composition.section.where($this is emptyReason).exists()"
        />
      </constraint>
      <mustSupport value="true"/>
    </element>
    <element id="Composition.section:immunisations.title">
      <path value="Composition.section.title"/>
      <min value="1"/>
      <fixedString value="Immunisations"/>
      <mustSupport value="true"/>
    </element>
    <element id="Composition.section:immunisations.code">
      <path value="Composition.section.code"/>
      <min value="1"/>
      <fixedCodeableConcept>
        <coding>
          <system value="http://loinc.org"/>
          <code value="11369-6"/>
        </coding>
      </fixedCodeableConcept>
      <mustSupport value="true"/>
    </element>
    <element id="Composition.section:immunisations.text">
      <path value="Composition.section.text"/>
      <min value="1"/>
      <mustSupport value="true"/>
    </element>
    <element id="Composition.section:immunisations.mode">
      <path value="Composition.section.mode"/>
      <fixedCode value="snapshot"/>
    </element>
    <element id="Composition.section:immunisations.entry">
      <path value="Composition.section.entry"/>
      <short value="Administered and/or non-administered vaccines"/>
      <type>
        <code value="Reference"/>
        <targetProfile
          value="http://ns.electronichealth.net.au/ci/fhir/3.0/StructureDefinition/immunization-summary-1"
        />
      </type>
      <type>
        <code value="Reference"/>
        <targetProfile
          value="http://ns.electronichealth.net.au/ci/fhir/3.0/StructureDefinition/observation-norelevantfinding-1"
        />
      </type>
      <constraint>
        <key value="inv-dh-cmp-05"/>
        <severity value="error"/>
        <human
          value="At least one statement of vaccine or an assertion that there is no history of immunisation shall be present, but not both"/>
        <expression
          value="Composition.section.where($this.code.coding.system='http://loinc.org' and $this.code.coding.code = '11369-6').exists() implies (Composition.section.where($this.code.coding.system='http://loinc.org' and $this.code.coding.code = '11369-6').entry.resolve().where($this is Immunization).exists() xor Composition.section.where($this.code.coding.system='http://loinc.org' and $this.code.coding.code = '11369-6').entry.resolve().where($this is Observation ).exists())"
        />
      </constraint>
      <constraint>
        <key value="inv-dh-cmp-11" />
