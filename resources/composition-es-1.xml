        <code value="Extension"/>
        <profile value="http://hl7.org.au/fhir/StructureDefinition/composition-author-role"/>
      </type>
      <constraint>
        <key value="inv-dh-cmp-02"/>
        <severity value="error"/>
        <human
          value="A practitioner role shall conform to PractitionerRole with Practitioner with Mandatory Identifier"/>
        <expression
          value="Composition.extension('http://hl7.org.au/fhir/StructureDefinition/composition-author-role').resolve().conformsTo('http://ns.electronichealth.net.au/ci/fhir/3.0/StructureDefinition/practitionerrole-withpractitionerident-1')"
        />
      </constraint>
      <mustSupport value="true"/>
    </element>
    <element id="Composition.identifier">
      <path value="Composition.identifier"/>
      <mustSupport value="true"/>
    </element>
    <element id="Composition.status">
      <path value="Composition.status"/>
      <mustSupport value="true"/>
    </element>
    <element id="Composition.type">
      <path value="Composition.type"/>
      <fixedCodeableConcept>
        <coding>
          <system value="http://loinc.org"/>
          <code value="34133-9"/>
        </coding>
      </fixedCodeableConcept>
      <mustSupport value="true"/>
    </element>
    <element id="Composition.subject">
