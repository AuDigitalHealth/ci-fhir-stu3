  <fhirVersion value="3.0.1"/>
  <kind value="resource"/>
  <abstract value="false"/>
  <type value="Composition"/>
  <baseDefinition value="http://hl7.org.au/fhir/StructureDefinition/au-composition"/>
  <derivation value="constraint"/>
  <differential>
    <element id="Composition">
      <path value="Composition"/>
      <short value="Event Summary"/>
      <definition
        value="An event summary authored by a nominated provider, that contains key pieces of information about an individual’s health status and is useful to a wide range of providers in assessing individuals and delivering care."
      />
    </element>
    <element id="Composition.meta">
      <path value="Composition.meta"/>
      <min value="1"/>
      <constraint>
        <key value="inv-dh-cmp-01"/>
        <severity value="error"/>
        <human
          value="One meta.profile shall be 'http://ns.electronichealth.net.au/ci/fhir/3.0/StructureDefinition/composition-es-1'"/>
        <expression
          value="Composition.meta.profile.where($this='http://ns.electronichealth.net.au/ci/fhir/3.0/StructureDefinition/composition-es-1').exists()"
        />
      </constraint>
    </element>
    <element id="Composition.meta.profile">
      <path value="Composition.meta.profile"/>
      <min value="1"/>
    </element>
    <element id="Composition.language">
      <path value="Composition.language"/>
      <min value="1"/>
      <max value="1"/>
      <fixedCode value="en-AU"/>
    </element>
    <element id="Composition.extension:compositionAuthorRole">
      <path value="Composition.extension"/>
      <sliceName value="compositionAuthorRole"/>
      <short value="Practitioner role that authored the composition"/>
      <min value="1"/>
      <max value="1"/>
