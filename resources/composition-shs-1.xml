  <differential>
    <element id="Composition">
      <path value="Composition" />
      <short value="Shared Health Summary" />
      <definition value="A health summary authored by a nominated provider, that contains key pieces of information about an individual’s health status and is useful to a wide range of providers in assessing individuals and delivering care." />
    </element>
