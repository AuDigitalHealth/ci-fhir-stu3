        <expression value="profile.where($this='http://ns.electronichealth.net.au/ci/fhir/3.0/StructureDefinition/composition-shs-1').exists()" />
      </constraint>
    </element>
    <element id="Composition.meta.profile">
      <path value="Composition.meta.profile" />
      <min value="1" />
    </element>
    <element id="Composition.language">
      <path value="Composition.language" />
