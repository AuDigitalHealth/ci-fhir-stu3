<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <id value="documentreference-acp-1" />
  <url value="http://ns.electronichealth.net.au/ci/fhir/3.0/StructureDefinition/documentreference-acp-1" />
  <version value="1.0.0" />
  <name value="DocumentReferenceACP" />
  <title value="Advance Care Planning Document Reference" />
  <status value="draft" />
  <experimental value="true"/>
  <publisher value="Australian Digital Health Agency" />
  <contact>
    <telecom>
      <system value="email" />
      <value value="help@digitalhealth.gov.au" />
      <use value="work" />
    </telecom>
  </contact>
  <description value="The purpose of this profile is to define a representation of a document reference of an attached Advance Care Plan for a patient in an Australian healthcare context. The document reference is intended to be suited to handling and exchange as a standalone record or as a referenced entry in a Composition.&#xa;&#xa;#### Must Support&#xa;&#xa;In the context of this profile [Must Support](http://hl7.org/fhir/STU3/conformance-rules.html#mustSupport) SHALL be interpreted as follows:&#xa;&#xa;* The system SHALL be able to include the element when authoring a resource where data is available&#xa;&#xa;* The system SHALL be able to store and retrieve the element&#xa;&#xa;* The system SHALL be able to process resources containing the element" />
  <copyright value="Copyright Â© 2019 Australian Digital Health Agency - All rights reserved. This content is licensed under a Creative Commons Attribution 4.0 International License. See https://creativecommons.org/licenses/by/4.0/." />
  <fhirVersion value="3.0.1" />
  <kind value="resource" />
  <abstract value="false" />
  <type value="DocumentReference" />
  <baseDefinition value="http://hl7.org/fhir/StructureDefinition/DocumentReference" />
  <derivation value="constraint" />
  <differential>
    <element id="DocumentReference">
      <path value="DocumentReference" />
      <short value="A reference to an advance care plan document" />
      <constraint>
        <key value="inv-dh-docref-02" />
        <severity value="error" />
        <human value="If the author is a practitioner, the role shall be provided" />
        <expression value="(DocumentReference.author.resolve() is Practitioner) implies DocumentReference.extension('http://hl7.org.au/fhir/StructureDefinition/author-role').exists()" />
      </constraint>
    </element>
    <element id="DocumentReference.meta">
      <path value="DocumentReference.meta" />
      <min value="1" />
      <constraint>
        <key value="inv-dh-docref-01" />
        <severity value="error" />
        <human value="One meta.profile shall be 'http://ns.electronichealth.net.au/ci/fhir/3.0/StructureDefinition/documentreference-acp-1'" />
        <expression value="DocumentReference.meta.profile.where($this='http://ns.electronichealth.net.au/ci/fhir/3.0/StructureDefinition/documentreference-acp-1').exists()" />
      </constraint>
    </element>
    <element id="DocumentReference.meta.profile">
      <path value="DocumentReference.meta.profile" />
      <min value="1" />
    </element>
    <!--<element id="DocumentReference.extension">
      <path value="DocumentReference.extension" />
      <slicing>
        <discriminator>
          <type value="value" />
          <path value="url" />
        </discriminator>
        <rules value="open" />
      </slicing>
    </element>-->
    <element id="DocumentReference.extension:AuthorRole">
      <path value="DocumentReference.extension" />
      <sliceName value="AuthorRole" />
      <short value="Practitioner role that authored the attached document" />
      <min value="0" />
      <max value="1" />
      <type>
        <code value="Extension" />
        <profile value="http://hl7.org.au/fhir/StructureDefinition/author-role" />
      </type>
      <constraint>
        <key value="inv-dh-docref-03"/>
        <severity value="error"/>
        <human value="A practitioner role shall conform to PractitionerRole with Practitioner with Mandatory Identifier"/>
        <expression value="DocumentReference.extension('http://hl7.org.au/fhir/StructureDefinition/author-role').resolve().conformsTo('http://ns.electronichealth.net.au/ci/fhir/3.0/StructureDefinition/practitionerrole-withpractitionerident-1')"/>
      </constraint>
      <mustSupport value="true"/>
    </element>
    <element id="DocumentReference.masterIdentifier">
      <path value="DocumentReference.masterIdentifier" />
      <mustSupport value="true" />
    </element>
    <element id="DocumentReference.identifier">
      <path value="DocumentReference.identifier" />
      <mustSupport value="true" />
    </element>
    <element id="DocumentReference.status">
      <path value="DocumentReference.status" />
      <fixedCode value="current"/>
      <mustSupport value="true" />
    </element>
    <element id="DocumentReference.docStatus">
      <path value="DocumentReference.docStatus" />
      <fixedCode value="final"/>
      <mustSupport value="true" />
    </element>
    <element id="DocumentReference.type">
      <path value="DocumentReference.type" />
      <short value="Advance Care Planning Document" />
      <fixedCodeableConcept>
        <coding>
          <system value="https://healthterminologies.gov.au/fhir/CodeSystem/nctis-data-components-1"/>
          <code value="100.16998"/>
        </coding>
      </fixedCodeableConcept>
      <mustSupport value="true"/>
    </element>
    <element id="DocumentReference.subject">
      <path value="DocumentReference.subject" />
      <min value="1" />
      <type>
        <code value="Reference" />
        <targetProfile value="http://ns.electronichealth.net.au/ci/fhir/3.0/StructureDefinition/patient-ident-1" />
      </type>
      <mustSupport value="true" />
    </element>
    <element id="DocumentReference.created">
      <path value="DocumentReference.created" />
      <short value="Time at which the attached advance care plan was created" />
      <min value="1"/>
      <mustSupport value="true" />
    </element>
    <element id="DocumentReference.indexed">
      <path value="DocumentReference.indexed" />
      <short value="Time at which this document reference was created" />
      <mustSupport value="true" />
    </element>
    <element id="DocumentReference.author">
      <path value="DocumentReference.author" />
      <short value="Author of the attached advance care plan" />
      <min value="1" />
      <max value="1" />
      <type>
        <code value="Reference" />
        <targetProfile value="http://ns.electronichealth.net.au/ci/fhir/3.0/StructureDefinition/practitioner-ident-1" />
      </type>
      <type>
        <code value="Reference" />
        <targetProfile value="http://ns.electronichealth.net.au/ci/fhir/3.0/StructureDefinition/patient-ident-1" />
      </type>
      <type>
        <code value="Reference" />
        <targetProfile value="http://ns.electronichealth.net.au/ci/fhir/3.0/StructureDefinition/relatedperson-ident-1" />
      </type>
      <mustSupport value="true"/>
    </element>
    <element id="DocumentReference.description">
      <path value="DocumentReference.description" />
      <mustSupport value="true" />
    </element>
    <element id="DocumentReference.content">
      <path value="DocumentReference.content" />
      <max value="1" />
      <mustSupport value="true" />
    </element>
    <element id="DocumentReference.content.attachment">
      <path value="DocumentReference.content.attachment" />
      <mustSupport value="true" />
    </element>
  </differential>
</StructureDefinition>
